definition
	EVALUATION[at0000] matches {	-- Vedtak om opphor tvungen obs
		data matches {
			ITEM_TREE[at0001] matches {	-- Tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0005] occurrences matches {0..1} matches {	-- Under tvungen observasjon siden
						value matches {
							DV_DATE matches {*}
						}
					}
					ELEMENT[at0006] occurrences matches {0..1} matches {	-- Begrunnelse for tvungen observasjon i journalnotat av
						value matches {
							DV_DATE matches {*}
						}
					}
					ELEMENT[at0007] occurrences matches {0..1} matches {	-- Begrunnelse for opphør av tvungen observasjon i journalnotat av
						value matches {
							DV_DATE matches {*}
						}
					}
					ELEMENT[at0017] occurrences matches {0..1} matches {	-- Det er fremmet begjæring
						value matches {
							DV_BOOLEAN matches {
								value matches {True}
							}
						}
					}
					ELEMENT[at0010] occurrences matches {0..1} matches {	-- Pasienten er gitt anledning til å uttale seg
						value matches {
							DV_BOOLEAN matches {
								value matches {True}
							}
						}
					}
					ELEMENT[at0011] occurrences matches {0..1} matches {	-- Nærmeste pårørendes uttalerett, jf. psykisk helsevernloven § 3-9
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0012, 	-- Pasienten har ingen personer med status som nærmeste pårørende som kunne gis anledning til å utale seg
									at0013, 	-- Nærmeste pårørende er ikke gitt anledning til å uttale seg da dennes adresse er ukjent og ettersporing vil kreve urimelig mye tid eller arbeid
									at0014]	-- Nærmeste pårørende er gitt anledning til å uttale seg
								}
							}
						}
					}
					ELEMENT[at0015] occurrences matches {0..1} matches {	-- Den som har fremsatt begjæring etter psykisk helsevernloven § 3-6, er gitt anledning til å uttale seg, jf. psykisk helsevernloven § 3-9
						value matches {
							DV_BOOLEAN matches {
								value matches {True}
							}
						}
					}
					ELEMENT[at0008] occurrences matches {0..1} matches {	-- Pasienten er informert om vedtaket
						value matches {
							DV_BOOLEAN matches {
								value matches {True}
							}
						}
					}
					ELEMENT[at0016] occurrences matches {0..1} matches {	-- Begjærende instans er informert om om sin klageadgang
						value matches {
							DV_BOOLEAN matches {
								value matches {True}
							}
						}
					}
					ELEMENT[at0009] occurrences matches {0..*} matches {	-- Iverksettinge besluttet utsatt
						value matches {
							DV_BOOLEAN matches {
								value matches {True}
							}
						}
					}
					ELEMENT[at0004] occurrences matches {0..1} matches {	-- Helsetjenesteenhet
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0003] occurrences matches {0..1} matches {	-- Dato vedtak fattet
						value matches {
							DV_DATE_TIME matches {
								value matches {yyyy-mm-ddTHH:MM:SS}
							}
						}
					}
					allow_archetype CLUSTER[at0002] occurrences matches {0..*} matches {	-- Vedtakstype
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

